{
  "group": "{{firstName}}",
  "count": "{{number max=80}}",
  "count": "{{number min=10 max=80}}",
  "mount": "{{number 10 80}}",
  "alive": "{{boolean}}",
  "tags{{repeat 10}}": [
    "hello"
  ],
  "tags{{10}}": [
    "hello"
  ],
  "people[5-10]": [
    {
      "name": "{{firstName}}"
    }
  ],


  "thing": "{{#repeat 3}}10 {{/repeat}}",
  "people": [
    {{#repeat 3}}
    {
      "id": {{index}},
      "name": "{{firstName}}",
      "email": "{{email}}",
      "age": {{number 20 50}}, // allow user to decide whether to surround in quotes
      "figures": [{{#repeat 3}}[1,2,3]{{/repeat}}]
    }
    {{/repeat}}
  ],
  "tags": [
    {{#repeat 3}}
    "{{firstName}}"
    {{/repeat}}
  ],
  "things": {{#array 5}}
    "stuff"
  {{/array}}
}