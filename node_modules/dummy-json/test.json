{
  "people": [
    {{#repeat 2}}
    {
      "id": {{index}},
      "firstName": "{{firstName}}",
      "lastName": "{{lastName}}",
      "email": "{{email}}",
      "work": "{{company}}",
      "age": {{number 20 50}},
      "optedin": {{boolean}}
    }
    {{/repeat}}
  ],
  "images": [
    {{#repeat 3 6}}
    'img{{index}}.png'
    {{/repeat}}
  ],
  "revision": {{uniqueIndex}},
  "tolerance": {{number '0.5' '1.9'}},
  "date range": "{{date '2015-01-01' '2015-10-10'}}",
  "date empty": "{{date}}",
  "time range": "{{time '09:00' '17:30'}}"
  "time empty": "{{time}}"
}